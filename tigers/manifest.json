{
    "name": "Tigers",
    "description": "Tigers is No.1!",
    "version": "0.1",
    "manifest_version": 3,
    "content_scripts": [
    { 
        "matches" : ["https://baseball.yahoo.co.jp/npb/standings/*"],
        "js" : ["content_script.js"]
    },
    {
      "matches" : ["<all_urls>"],
      "js" : ["content_script3.js"],
      "css" : ["mystyle2.css"]
    }
    ],
    "web_accessible_resources": [
      {
        "resources": [
          "*.mp3"
        ],
        "matches": [
          "<all_urls>"
        ]
      }
    ]
}